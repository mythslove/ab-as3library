package net.guttershark.util.filters{	/**	 * FilterType defines the rendering type of a Filter object.	 */	public class FilterType 	{		/**		 * Defines the filter type applied to the area of an object.		 */		public static const FULL:String = "full";		/**		 * Defines the filter type applied to the inner area of an object.		 */		public static const INNER:String = "inner";		/**		 * Defines the filter type applied to the outer area of an object.		 */		public static const OUTER:String = "outer";	}}