package net.guttershark.util.collections{	/**	 * ICollection defines an object which represents a collection of objects.	 */	public interface ICollection 	{		/**		 * The iterator used to handle going through the collection.		 */		function getIterator():IIterator;				/**		 * Removes all items from the collection.		 */		function clear():void;				/**		 * How many items exist within the collection.		 */		function get count():uint;		/**		 * Whether or not this collection is empty.		 */		function get isEmpty():Boolean;	}}