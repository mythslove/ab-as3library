package net.guttershark.util.collections{	import net.guttershark.util.collections.IBidirectionalIterator;	/**	 * ILoopableIterator defines how loopable, nondestructive, bi-directional iterating of collections of items is handled.	 */	public interface ILoopableIterator extends IBidirectionalIterator 	{		/**		 * Returns <code>true</code> if we are at the start of the collection.		 */		function isAtStart():Boolean;		/**		 * Returns <code>true</code> if we are at the end of the collection.		 */		function isAtEnd():Boolean;	}}