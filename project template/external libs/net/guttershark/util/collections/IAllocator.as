package net.guttershark.util.collections{	/**	 * IAllocator provides a common interface for the <code>Allocator</code> object and its descendants.	 */	public interface IAllocator 	{		/**		 * Retrieves an object from the cache. If an object does not 		 * exist at the current cache index, a new object is created.		 */		function getObject():*		/**		 * Releases all cached objects from memory.		 */		function release():void;		/**		 * Resets the cache index.		 */		function reset():void;	}}